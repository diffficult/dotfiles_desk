# rsync
abbr "rsync-copy"="rsync --verbose --progress --human-readable --compress --archive --hard-links --one-file-system"
abbr "rsync-move"="rsync --verbose --progress --human-readable --compress --archive --hard-links --one-file-system --remove-source-files"
abbr "rsync-sync"="rsync --verbose --progress --human-readable --compress --archive --hard-links --one-file-system --update --delete"
abbr "rsync-update"="rsync --verbose --progress --human-readable --compress --archive --hard-links --one-file-system --update"

